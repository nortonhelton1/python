tasks = []

while True:

  print("1 - Add Task")
  print("2 - Delete Task")
  print("3 - View all tasks")
  print("q - Quit")

  choice = int(input("Enter choice"))

  if choice == "q":
    break

  if choice == 1:
    title = input("Please enter choice title")
    priority = input("Please enter task priority")
    task = {"title": title, "priority": priority}
    task.append(task)

    print(task)
        
  if choice == 3:
      print (tasks)
